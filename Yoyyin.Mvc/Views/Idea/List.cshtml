@using Yoyyin.Mvc.Helpers
@model IEnumerable<Yoyyin.Mvc.Models.UserWithFirstIdea>

@Html.Partial("Breadcrumb")
@Html.PageHeader((string)@ViewBag.Title, (string)@ViewBag.SubTitle)

<div class="ui-helper-clearfix">
    @foreach (var user in Model)
    {
        @Html.Partial("~/Views/Idea/UserRow.cshtml", user)
    }
</div>




