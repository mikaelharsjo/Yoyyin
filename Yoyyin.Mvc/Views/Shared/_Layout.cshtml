<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - Yoyyin - Sammanför människor med idéer</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <link rel="stylesheet/less" type="text/css" href="/Content/Less/site.less">
    <link rel="stylesheet/less" type="text/css" href="/Content/Less/bootstrap.less">
    <link href="/Content/Css/jquery-ui-1.8.16.custom.css" rel="stylesheet" type="text/css" />
    <script src="/Scripts/less-1.3.0.min.js" type="text/javascript"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.7.2/jquery-ui.min.js"></script>
    <script src="http://maps.google.com/maps/api/js?sensor=false"></script>
    <script src="/Scripts/jquery.gomap-1.3.2.min.js" type="text/javascript"></script>
    <script src="/Scripts/bootstrap.js" type="text/javascript"></script>
    <script src="/Scripts/mustache.js" type="text/javascript"></script>
    <script src="/Scripts/sammy.js" type="text/javascript"></script>
    <script src="/Scripts/yoyyin/yoyyin.init.js" type="text/javascript"></script>
    <script src="/Scripts/yoyyin/register/location.js" type="text/javascript"></script>
    <script src="/Scripts/yoyyin/register/register.js" type="text/javascript"></script>
    <script src="/Scripts/yoyyin/yoyyin.js" type="text/javascript"></script>
    <script src="/Scripts/yoyyin/yoyyin.quickSearch.js" type="text/javascript"></script>
    <meta name="viewport" content="width=device-width" />
</head>
<body>
    <header>
            <div class="content-wrapper">
                    <nav class="navbar navbar-fixed-top">
                        <div class="navbar-inner">
                            <div class="container">
                                <a class="brand" href="#">Yoyyin</a>
                                <ul class="nav">
                                    <li>@Html.ActionLink("Hem", "Index", "Home")</li>
                                    <li>@Html.ActionLink("Om", "About", "Home")</li>
                                    <li>@Html.ActionLink("Kontakt", "Contact", "Home")</li>
                                    <li>@Html.ActionLink("Diskutera", "Index", "QandA")</li>
                                    <li>@Html.ActionLink("Länkar", "Links", "Home")</li>
                                    <li class="dropdown">
                                        <a class="dropdown-toggle" data-toggle="dropdown">Affärsidéer</a>
                                        <ul class="dropdown-menu">
                                            <li><a href="/User/List">Alla</a></li>     
                                            <li><a href="/User/List">per bransch</a></li>
                                            <li><a href="/Tagging/ListCompetences">per kompetens</a></li>                                            
                                            <li><a href="/user/ListWantsFinancing">som söker finansiering</a></li>
                                        </ul>
                                    </li>
                                </ul>  

                                    <form id="formQuickSearch" class="navbar-search pull-left" onkeypress="return event.keyCode != 13">
                                        <input id="txtSearch" type="text" class="search-query" placeholder="Sök affärspartner/kompetens mm">
                                    </form>  
                                <ul class="nav pull-right">
                                    @Html.Partial("_LoginPartial")
                                </ul>
                            </div>
                        </div>
                    </nav>
            </div>
        </header>
    <div id="body">
        @*What is this*@
        @RenderSection("featured", required: false)
        <section id="sectionMainContent" class="content-wrapper main-content clear-fix">
            @RenderBody()
        </section>
    </div>
    <footer>
            <div class="content-wrapper">
                <div class="footerCopy">
                    <p>&copy; @DateTime.Now.Year - Yoyyin Ventures</p>
                </div>
                <div class="footerSocial">
                    <ul id="social">
                        <li><a href="http://facebook.com" class="facebook">Facebook</a></li>
                        <li><a href="http://twitter.com" class="twitter">Twitter</a></li>
                    </ul>
                </div>
            </div>
        </footer>
</body>
</html>
